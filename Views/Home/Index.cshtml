@model List<hospital.ViewModels.RowViewModel>

<table class="table table-dark shadow mt-5 mb-3 border border-5 border-light rounded-3">
  <thead>
    <tr>
      <th scope="col">Ф.И.О</th>
      <th scope="col">Телефон Номер</th>
      <th scope="col">Ваша очередь</th>
    </tr>
  </thead>
  <tbody>

    @foreach (var user in Model)
    {
      <tr>
        <td>@user.Fullname</td>
        <td>(XX) XXX @user.Phone.Substring(user.Phone.Length - 4)</td>
        <td>@user.CreatedAt.ToString("MMM dd, yyyy HH:mm")</td>
      </tr>
    }
  </tbody>
</table>